  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "precommit": "lint-staged",
    "clean": "gatsby clean",
    "start": "npm run develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "npm run clean && gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:lint": "eslint src -c .eslintrc --ext js,jsx && prettier --list-different --ignore-path .eslintignore \"**/*.+(jsx|js|scss|css)\"",
    "test:lint:fix": "eslint --ignore-path .eslintignore . --fix && prettier --loglevel warn  --ignore-path .eslintignore --write \"**/*.+(jsx|js|scss|css)\"",
    "stylelint": "stylelint '**/*.scss' --config=.stylelintrc.json"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-sass": "^2.1.27",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^1.19.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --config=.stylelintrc.json --fix",
      "prettier --config .prettierrc --write"
    ]
  }
}
